{
  "name": "deepunit",
  "version": "1.1.0",
  "description": "In beta. support@deepunit.ai",
  "main": "dist/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "start": "ts-node src/main.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "test": "jest",
    "push": "npm run prep && npm publish",
    "tar": "rm -rf dist/* ; npm run build && chmod +x dist/postInstall.js && npm pack",
    "postinstall": "dist/postInstall.js"
  },
  "keywords": [],
  "author": "DeepUnit.AI",
  "license": "http://deepunit.ai/license",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1"
  },
  "bin": "./dist/main.js",
  "files": [
    "dist/*",
    "deepunit.config.json"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "typescript": "^5.1.6"
  },
  "compilerOptions": {
    "esModuleInterop": true
  }
}
