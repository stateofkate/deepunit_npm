{
  "name": "deepunit",
  "version": "1.4.0",
  "description": "In beta. support@deepunit.ai",
  "main": "dist/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "rm -rf dist/* ; tsc",
    "start": "ts-node src/main.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "test": "jest",
    "test:cov": "npx jest --coverage --verbose --runInBand --passWithNoTests",
    "deleteTests": "find . -type f -name \"*.test.ts\" -exec rm -f {} \\;\n",
    "deleteTempTests": "find . -type f -name \"*.deepunittemptest.test.ts\" -exec rm -f {} \\;\n",
    "summary": "npx jest src --colors 2>&1 | awk '/FAIL/ {print \"\\033[31m\" $0 \"\\033[0m\"} /PASS/ {print \"\\033[32m\" $0 \"\\033[0m\"}'",
    "push": "npm run build && npm publish",
    "tar": "npm run build && npm pack"
  },
  "keywords": [],
  "author": "DeepUnit.AI",
  "license": "http://deepunit.ai/license",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1"
  },
  "bin": "./dist/main.js",
  "files": [
    "dist/*",
    "deepunit.config.json"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "deepunit": "^1.3.4",
    "typescript": "^5.1.6"
  },
  "compilerOptions": {
    "esModuleInterop": true
  }
}
